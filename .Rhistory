base<-read_csv("https://raw.githubusercontent.com/LeoEnriquez/TareasMacro2/Tarea-1/Datos%20Tarea%201/base.csv")
IPCc<- ts(base$IPCc, start=1990, frequency = 1)
ts.plot (IPCc,   xlab="Años", ylab="IPC cierre", main="Gráfica 5.1. Índice de Precios y Cotizaciones de la BMV", col="blue")
tc_IPCc<-growth.rate(IPCc, lag=1)
ts.plot (tc_IPCc,   xlab="Años", ylab="Tasa de retorno del IPC cierre", main="Gráfica 5.2. Tasa de retorno del IPC de la BMV", col="blue")
CETES28<- ts(base$CETES28, start=1990, frequency = 1)
CETES364<-ts(base$CETES364, start=1990, frequency = 1)
ts.plot (CETES28,CETES364,  gpars=list(xlab="Años", ylab="Tasas de interés", main="Gráfica 5.3. Tasas de interés de CETES a 28 y 364 días", col=c("blue","red"),cex.main=0.9))
legend ('topright', col=c("blue","red"),lwd=2, legend=c("CETES 28","CETES 364"), bg='white')
basec<-base[-1,]
d_IPCCETES28<-tc_IPCc-basec$CETES28
d_IPCCETES364<-tc_IPCc-basec$CETES364
d_IPCCETES28<-ts(d_IPCCETES28, start=1991, frequency = 1)
d_IPCCETES364<-ts(d_IPCCETES364, start=1991, frequency = 1)
ts.plot (d_IPCCETES28,d_IPCCETES364,  gpars=list(xlab="Años", ylab="Diferencial de retornos", main=" Gráfica 5.4. Diferencial de retornos del IPC con respecto \n a CETES a 28 y 364 días", col=c("blue","red"),cex.main=0.9))
legend ('topright', col=c("blue","red"),lwd=2, legend=c("Diferencial con CETES 28","Diferencial con CETES 364"), bg='white')
Cons<- ts(base$Cons, start=1990, frequency = 1)
tc_Cons <- growth.rate(Cons, lag=1)
ts.plot (tc_Cons,   xlab="Años", ylab="Tasa de crecimiento", main="Gráfica 5.5. Tasa de crecimiento del Consumo", col="blue")
#Desviación estándar del crecimiento del consumo, divido por 100 para cambiar de escala para decimales como en Romer
sd_tcCons<-(sd(tc_Cons))/100
#sd_tcCons
#Desviación estándar del diferencial de la tasa de CETES28 y la tasa de crecimiento del IPCc como indicador de retorno der mercado de valores. Misma obs para la división de 100
sd_dif_IPC_CETES28<-(sd(d_IPCCETES28))/100
#sd_dif_IPC_CETES28
#Correlación de tasa de crecimiento del consumo y el diferencial de rendimientos
corr_sd<-cor(tc_Cons, d_IPCCETES28)
#corr_sd
#La covarianza entre la tasa de crecimiento del consumo y el diferencial de rendimientos.Coto plazo
cov_C_CETES28<-sd_tcCons*sd_dif_IPC_CETES28*corr_sd
#cov_C_CETES28
##################
#Desviación estándar del crecimiento del consumo, divido por 100 para cambiar de escala para decimales como en Romer
sd_tcCons<-(sd(tc_Cons))/100
#sd_tcCons
#Desviación estándar del diferencial de la tasa de CETES364 y la tasa de crecimiento del IPCc como indicador de retorno der mercado de valores. Misma obs para la división de 100
sd_dif_IPC_CETES364<-(sd(d_IPCCETES364))/100
#sd_dif_IPC_CETES364
#Correlación de tasa de crecimiento del consumo y el diferencial de rendimientos
corr_sd_lp<-cor(tc_Cons, d_IPCCETES364)
#corr_sd_lp
#La covarianza entre la tasa de crecimiento del consumo y el diferencial de rendimientos.Coto plazo
cov_C_CETES364<-sd_tcCons*sd_dif_IPC_CETES364*corr_sd_lp
#cov_C_CETES364
##################
##################
#Retomando la covarianza
#cov_C_CETES28
#Tomamos la media como la esperanza de los CETES y del rendimeinto del IPC.
mean_CETES28<-mean(basec$CETES28)
mean_tc_IPCc<-mean(tc_IPCc)
#Diferencial promedio del rendimiento del IPC y del rendimientos de CETES28, es decir es el premio al capital o exceso de rendimiento
difermean_IPC_cetes28<-(mean_tc_IPCc-mean_CETES28)/100
#difermean_IPC_cetes28
#Coeficiente de aversión relativa al riesgo (teta)
tetacp<-difermean_IPC_cetes28/((cov_C_CETES28))
#tetacp
##################
#Retomando la covarianza
#cov_C_CETES364
#Tomamos la media como la esperanza de los CETES y del rendimeinto del IPC.
mean_CETES364<-mean(basec$CETES364)
mean_tc_IPCc<-mean(tc_IPCc)
#Diferencial promedio del rendimiento del IPC y del rendimientos de CETES364, es decir es el premio al capital o exceso de rendimiento
difermean_IPC_cetes364<-(mean_tc_IPCc-mean_CETES364)/100
#difermean_IPC_cetes364
#Coeficiente de aversión relativa al riesgo (teta)
tetalp<-difermean_IPC_cetes364/((cov_C_CETES364))
