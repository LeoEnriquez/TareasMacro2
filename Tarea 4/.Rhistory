captaciones3 <- read_csv("https://raw.githubusercontent.com/LeoEnriquez/TareasMacro2/main/Tarea%204/Datos/Ejercicio%203_captaciones.csv")
captaciones3<- ts(captaciones3, start=1986, frequency = 1)
ts.plot (captaciones3[,2], captaciones3[,3], captaciones3[,4], captaciones3[,5], captaciones3[,6], captaciones3[,7], captaciones3[,8], captaciones3[,9], captaciones3[,10], gpars=list(xlab="Años", ylab="Valor", main=" Gráfica 3.1. Niveles absolutos de los instrumentos \n de financiación de la banca comercial, 1986-2020", col=c("black", "green","blue","cyan", "yellow", "orange", "purple", "brown", "red"),lwd= 1, cex.main=0.9))
